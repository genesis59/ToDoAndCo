{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "eightpoints/guzzle-bundle": "^v8.4.0",
        "guzzlehttp/guzzle": "^6.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "symfony/asset": "5.0.0",
        "symfony/cache": "5.0.0",
        "symfony/console": "5.0.0",
        "symfony/dotenv": "5.0.0",
        "symfony/flex": "^1.20",
        "symfony/form": "5.0.0",
        "symfony/framework-bundle": "5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "5.0.0",
        "symfony/security-csrf": "5.0.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "5.0.0",
        "symfony/twig-bundle": "5.0.0",
        "symfony/validator": "5.0.0",
        "symfony/yaml": "5.0.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.0.0",
        "symfony/stopwatch": "5.0.0",
        "symfony/web-profiler-bundle": "5.0.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.0",
            "docker": true
        }
    }
}
